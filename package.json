{
  "name": "empire-premier-app",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=.env expo start",
    "android": "NODE_ENV=.env expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "build-android-staging": "export NODE_ENV=.env.staging && cd android && ./gradlew assembleRelease && cd ..",
    "build-android-prod": "export NODE_ENV=.env.prod && cd android && ./gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.4.0",
    "expo": "^49.0.8",
    "expo-image-picker": "~14.3.2",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-native": "0.72.4",
    "react-native-autocomplete-dropdown": "^3.0.2",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "~3.3.0",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-webview": "13.2.2",
    "react-native-youtube-iframe": "^2.3.0",
    "styled-components": "^5.3.11",
    "yup": "^1.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@expo/webpack-config": "^19.0.0",
    "@types/react": "~18.2.14",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "prettier": "^2.8.8",
    "react-native-dotenv": "^3.4.8",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "^5.1.3"
  },
  "private": true
}